<?xml version="1.0" standalone='no'?><!--*-nxml-*-->
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
  <name replace-wildcards="yes">%h</name>
  <service>
    <type>_rtp._udp</type>
    <port>5000</port>
    <txt-record>description=GStreamer H.264 Stream</txt-record>
    <txt-record>codec=H264</txt-record>
    <txt-record>bitrate=variable</txt-record>
  </service>
</service-group>